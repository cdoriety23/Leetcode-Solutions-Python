class Solution://Maxsubarry problem
    def maxSubArray(self, nums):
        currmax = nums[0]
        maxsum = nums[0]
        for i in range(1, len(nums)) :
            currmax = max(currmax + nums[i], nums[i])
            maxsum = max(maxsum, currmax)
        return maxsum
        

numbers=[1,-8,2,-100,2,5]
a = Solution()
numbers_2=[1,3,-4,2,4,-2,1,5]

print(a.maxSubArray(numbers))#7
print(a.maxSubArray(numbers_2))#10
